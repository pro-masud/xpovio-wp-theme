    <div class="product-tabel pt-5">
        <div class="container">
            <div class="row">
                <div class="col-lg-10 offset-lg-1">
                    <div class="card">
                        <div class="card-body">
                            <a class="btn btn-warning" href="/create">Create Product</a>
                            <table class="table table-striped">
                                <thead>
                                    <tr>
                                        <th>#</th>
                                        <th>Photo</th>
                                        <th>Name</th>
                                        <th>Regular Price</th>
                                        <th>Price</th>
                                        <th>Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% products.forEach((product, index) => { %>
                                        <tr>
                                            <td><%= index+1 %></td>
                                            <td><img style="width: 80px;" src="product/<%= product.productPhoto %>" alt=""></td>
                                            <td><%= product.name %></td>
                                            <td><%= product.regularPrice %></td>
                                            <td><%= product.selaPrice %></td>
                                            <td>
                                                <a class="btn btn-info" href="/single/<%= product.slug %>"><i class="fa-solid fa-eye"></i></a>
                                                <a class="btn btn-success" href="/edite/<%= product.id %>"><i class="fa-solid fa-pen-to-square"></i></a>
                                                <a class="btn btn-danger" href="/product-item/<%= product.id %>"><i class="fa-solid fa-trash"></i></a>
                                            </td>
                                        </tr>
                                    <% }) %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
 